<%= form_for @brew do |f| %>
  <%= f.label :blend_name %>
  <%= f.text_field :blend_name %>
  <%= f.label :origin %>
  <%= f.text_field :origin %>
  <%= f.label :notes %>
  <%= f.text_field :notes %>
  <%= f.label "1" %>
  <%= f.radio_button :strength, 1 %>
  <%= f.label "2" %>
  <%= f.radio_button :strength, 2 %>
  <%= f.label "3" %>
  <%= f.radio_button :strength, 3 %>
  <%= f.label "4" %>
  <%= f.radio_button :strength, 4 %>
  <%= f.label "5" %>
  <%= f.radio_button :strength, 5 %>
  <%= f.submit %>
<% end%>
